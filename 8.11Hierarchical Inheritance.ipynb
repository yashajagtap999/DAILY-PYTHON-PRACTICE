   "source": [
    "# Hierarchical inheritance\n",
    "class F():\n",
    "    def getf(self):\n",
    "        print(\"Father\")\n",
    "class C(F):\n",
    "    def getc(self):\n",
    "        print(\"Child\")\n",
    "class CC(F):\n",
    "    def getcc(self):\n",
    "        print(\"CC\")\n",
    "CC=CC()\n",
    "CC.getcc()\n",
    "CC.getf()"
   ]
