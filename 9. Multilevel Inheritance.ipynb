    "#Multilevel\n",
    "class P():\n",
    "    def base(self):\n",
    "        print(\"Base class\")\n",
    "class D(P):\n",
    "    def derived(self):\n",
    "        print(\"Derived class\")\n",
    "class DD(D):\n",
    "    def dderived(self):\n",
    "        print(\"DDerived class\")\n",
    "p1=P()\n",
    "d1=D()\n",
    "dd=DD()\n",
    "p1.base()\n",
    "dd.base()\n",
    "dd.derived()\n",
    "d1.base()"
   ]
